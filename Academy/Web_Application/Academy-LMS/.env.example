APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
COMPLIANCE_EXPORT_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_DOMAIN=localhost
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=strict

# Managed secret references (pulled at deploy time)
SECRETS_MANAGER_DRIVER=aws
SECRETS_MANAGER_PATH=academy/${APP_ENV}/laravel

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_CLUSTER=redis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB_APP=0
REDIS_DB_CACHE=1
REDIS_DB_HORIZON=2
REDIS_DB_BROADCASTING=3

# Queue topology
QUEUE_DEFAULT=default
QUEUE_NOTIFICATIONS=notifications
QUEUE_MEDIA=media
QUEUE_WEBHOOKS=webhooks
QUEUE_SEARCH=search-index
QUEUE_REDIS_CONNECTION=horizon
QUEUE_RETRY_AFTER=120
QUEUE_BLOCK_FOR=5

# Horizon scaling controls
HORIZON_BALANCE=auto
HORIZON_BALANCE_MAX_SHIFT=1
HORIZON_BALANCE_COOLDOWN=3
HORIZON_NOTIFICATIONS_MIN_PROCESSES=2
HORIZON_NOTIFICATIONS_MAX_PROCESSES=20
HORIZON_NOTIFICATIONS_CONCURRENCY=20
HORIZON_MEDIA_MIN_PROCESSES=1
HORIZON_MEDIA_MAX_PROCESSES=12
HORIZON_MEDIA_CONCURRENCY=12
HORIZON_WEBHOOKS_MIN_PROCESSES=1
HORIZON_WEBHOOKS_MAX_PROCESSES=8
HORIZON_WEBHOOKS_CONCURRENCY=6
HORIZON_SEARCH_MIN_PROCESSES=1
HORIZON_SEARCH_MAX_PROCESSES=8
HORIZON_SEARCH_CONCURRENCY=4
HORIZON_WORKER_MEMORY=256
HORIZON_WORKER_TIMEOUT=120
HORIZON_WORKER_TRIES=3
MAIL_HORIZON_NOTIFICATIONS_TO=
SLACK_HORIZON_WEBHOOK=

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

SECURITY_HEADERS_ENABLED=true

# Performance & caching
PERFORMANCE_QUERY_CACHE_ENABLED=true
PERFORMANCE_QUERY_CACHE_TTL=300
PERFORMANCE_QUERY_CACHE_ALLOW_DEBUG=false
PERFORMANCE_HTTP_CACHE_ENABLED=true
PERFORMANCE_HTTP_CACHE_MAX_AGE=60
PERFORMANCE_HTTP_CACHE_STALE=120
PERFORMANCE_HTTP_CACHE_SKIP_AUTH=true
PERFORMANCE_HTTP_CACHE_SKIP_COOKIE=true
PERFORMANCE_CACHE_WARM_ENABLED=false
PERFORMANCE_CACHE_WARM_SCHEDULE=02:00

# Octane runtime
OCTANE_SERVER=roadrunner
OCTANE_HTTPS=false
OCTANE_WORKERS=auto
OCTANE_MAX_REQUESTS=250
OCTANE_GARBAGE_THRESHOLD=64
OCTANE_MAX_EXECUTION_TIME=30
OCTANE_CACHE_ROWS=2048
OCTANE_CACHE_BYTES=32768
OCTANE_LEAK_GUARD=true
OCTANE_LEAK_GUARD_AUTH=true
OCTANE_LEAK_GUARD_TRANSLATOR=true
OCTANE_LEAK_GUARD_MODEL=true

# Search & discovery
SEARCH_DRIVER=meilisearch
MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_KEY=null
MEILISEARCH_TIMEOUT=10
