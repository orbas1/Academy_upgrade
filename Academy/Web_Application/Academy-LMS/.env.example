APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# MySQL performance tuning
DB_SESSION_SQL_MODE=STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_ENGINE_SUBSTITUTION
DB_INNODB_STRICT_MODE=ON
DB_INNODB_LOCK_WAIT_TIMEOUT=50
DB_INNODB_FLUSH_LOG_AT_TRX_COMMIT=1
DB_INNODB_STATS_PERSISTENT=ON
DB_MAX_EXECUTION_TIME=2000
DB_OPTIMIZER_SWITCH_INDEX_MERGE_INTERSECTION=off
DB_OPTIMIZER_SWITCH_INDEX_MERGE_UNION=off
DB_OPTIMIZER_SWITCH_INDEX_MERGE_SORT_UNION=off

# Keyset pagination defaults
KEYSET_PAGINATION_PER_PAGE=20
KEYSET_PAGINATION_MAX_PER_PAGE=100
KEYSET_PAGINATION_PAGE_PARAMETER=per_page
KEYSET_PAGINATION_CURSOR_NAME=cursor
KEYSET_PAGINATION_DEFAULT_COLUMN=id
KEYSET_PAGINATION_DEFAULT_DIRECTION=desc

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
COMPLIANCE_EXPORT_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_DOMAIN=localhost
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=strict

# Managed secret references (pulled at deploy time)
SECRETS_MANAGER_DRIVER=aws
SECRETS_MANAGER_PATH=academy/${APP_ENV}/laravel

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_CLUSTER=redis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB_APP=0
REDIS_DB_CACHE=1
REDIS_DB_HORIZON=2
REDIS_DB_BROADCASTING=3

# Queue topology
QUEUE_DEFAULT=default
QUEUE_NOTIFICATIONS=notifications
QUEUE_MEDIA=media
QUEUE_WEBHOOKS=webhooks
QUEUE_SEARCH=search-index
QUEUE_REDIS_CONNECTION=horizon
QUEUE_RETRY_AFTER=120
QUEUE_BLOCK_FOR=5

# Queue monitor thresholds
QUEUE_MONITOR_REDIS_CONNECTION=horizon
QUEUE_MONITOR_RETENTION_HOURS=168
QUEUE_MONITOR_DEFAULT_PENDING_THRESHOLD=150
QUEUE_MONITOR_DEFAULT_DELAYED_THRESHOLD=150
QUEUE_MONITOR_DEFAULT_RESERVED_THRESHOLD=150
QUEUE_MONITOR_DEFAULT_OLDEST_PENDING=180
QUEUE_MONITOR_DEFAULT_BACKLOG_DELTA=-5
QUEUE_MONITOR_MEDIA_PENDING=150
QUEUE_MONITOR_MEDIA_OLDEST_PENDING=120

# Horizon scaling controls
HORIZON_BALANCE=auto
HORIZON_BALANCE_MAX_SHIFT=1
HORIZON_BALANCE_COOLDOWN=3
HORIZON_NOTIFICATIONS_MIN_PROCESSES=2
HORIZON_NOTIFICATIONS_MAX_PROCESSES=20
HORIZON_NOTIFICATIONS_CONCURRENCY=20
HORIZON_MEDIA_MIN_PROCESSES=1
HORIZON_MEDIA_MAX_PROCESSES=12
HORIZON_MEDIA_CONCURRENCY=12
HORIZON_WEBHOOKS_MIN_PROCESSES=1
HORIZON_WEBHOOKS_MAX_PROCESSES=8
HORIZON_WEBHOOKS_CONCURRENCY=6
HORIZON_SEARCH_MIN_PROCESSES=1
HORIZON_SEARCH_MAX_PROCESSES=8
HORIZON_SEARCH_CONCURRENCY=4
HORIZON_WORKER_MEMORY=256
HORIZON_WORKER_TIMEOUT=120
HORIZON_WORKER_TRIES=3
MAIL_HORIZON_NOTIFICATIONS_TO=
SLACK_HORIZON_WEBHOOK=

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_MEDIA_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

MEDIA_DISK=media
MEDIA_CDN_URL=
MEDIA_IMAGE_BREAKPOINTS=320,640,960,1280
MEDIA_TRANSCODE_QUEUE=media
MEDIA_VISIBILITY=public
MEDIA_RESPONSIVE_PREFIX=responsive
MEDIA_TRANSCODE_PREFIX=transcoded

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

SECURITY_HEADERS_ENABLED=true

# Performance & caching
PERFORMANCE_QUERY_CACHE_ENABLED=true
PERFORMANCE_QUERY_CACHE_TTL=300
PERFORMANCE_QUERY_CACHE_ALLOW_DEBUG=false
PERFORMANCE_HTTP_CACHE_ENABLED=true
PERFORMANCE_HTTP_CACHE_MAX_AGE=60
PERFORMANCE_HTTP_CACHE_STALE=120
PERFORMANCE_HTTP_CACHE_SKIP_AUTH=true
PERFORMANCE_HTTP_CACHE_SKIP_COOKIE=true
PERFORMANCE_CACHE_WARM_ENABLED=false
PERFORMANCE_CACHE_WARM_SCHEDULE=02:00

# Octane runtime
OCTANE_SERVER=roadrunner
OCTANE_HTTPS=false
OCTANE_WORKERS=auto
OCTANE_MAX_REQUESTS=250
OCTANE_GARBAGE_THRESHOLD=64
OCTANE_MAX_EXECUTION_TIME=30
OCTANE_CACHE_ROWS=2048
OCTANE_CACHE_BYTES=32768
OCTANE_LEAK_GUARD=true
OCTANE_LEAK_GUARD_AUTH=true
OCTANE_LEAK_GUARD_TRANSLATOR=true
OCTANE_LEAK_GUARD_MODEL=true

# Search & discovery
SEARCH_DRIVER=meilisearch
MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_KEY=null
MEILISEARCH_TIMEOUT=10

# Messaging & Push Providers
PUSH_PROVIDER=fcm
FCM_REST_ENDPOINT=
FCM_REST_TOKEN=
RESEND_PUSH_ENDPOINT=
RESEND_PUSH_TOKEN=
MOBILE_DEEP_LINK_SCHEME=academy://

# Stripe Billing
STRIPE_SECRET=
STRIPE_WEBHOOK_SECRET=
STRIPE_WEBHOOK_TOLERANCE=300
